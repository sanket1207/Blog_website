<%- include('partials/header') -%>
    <h1>Home</h1>

    <p>
        <%= homeStartingContent %>
    </p>



    <% posts.forEach(function(post){ %>
        <h1>
            <%= post.postTitle %>
        </h1>
        <p>
            <%= post.postBody.substring(1, 100) + "..." %>
            <a href="/post/<%=post.postTitle%>">Read more</a>
        </p>
        <% }); %>



            <%- include('partials/footer') -%>